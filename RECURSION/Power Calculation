#include<iostream>
using namespace std;

long long int calcPower(int n, int power)
{
    if(power == 0) return 1;
    if(power == 1) return n;
    //recursive call
    int ans = calcPower(n,power/2);
    if(power % 2 == 0) return (ans*ans);  //if even power
    return (n*ans*ans);    //if odd power
}

int main() {
    int n,power;
    cout<<"Enter a number and its power: ";
    cin>>n>>power;
    cout<< calcPower(n,power);
    return 0;
}

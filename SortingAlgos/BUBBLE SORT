void bubbleSort(int arr[], int n)
{
    //basic idea: consecutive ele check karo agar first ele next ele se bada hai toh swap karo else no swaps
    for(int i=0 ;i<n-1;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(arr[i]>arr[j])
            {
                //swap them
                int temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
    return;
}
